    "relations",
    "typescript",
    "nestjs",
    "react",
    "angular",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ack-solutions/nest-crud"
  },
  "license": "MIT",
  "author": "Ackplus",
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.0.0"
  }
}
