{
  "name": "@ackplus/nest-crud",
  "version": "1.1.29",
  "description": "Powerful CRUD operations for NestJS with TypeORM - automatic REST endpoints, advanced filtering, relations, pagination, and more",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "nestjs",
    "crud",
    "typeorm",
    "rest",
    "api",
    "query-builder",
    "filtering",
    "pagination",
    "relations",
    "nestjs-crud",
    "nestjs-typeorm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ack-solutions/nest-crud"
  },
  "license": "MIT",
  "author": "Ackplus",
  "dependencies": {
    "pluralize": "^8.0.0",
    "deepmerge": "^4.3.1",
    "lodash": "^4.17.21",
    "qs": "^6.11.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^10 || ^11",
    "@nestjs/core": "^10 || ^11",
    "@nestjs/platform-express": "^10 || ^11",
    "@nestjs/swagger": "^10 || ^11",
    "@nestjs/typeorm": "^10 || ^11",
    "class-validator": "^0.14.1",
    "typeorm": "^0.3.21",
    "reflect-metadata": "^0.1.13",
    "class-transformer": "^0.5.1"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "@types/deepmerge": "^2.2.0",
    "@types/lodash": "^4.17.0",
    "@types/qs": "^6.9.12",
    "@types/node": "^22.0.0",
    "@types/jest": "^30.0.0"
  }
}
