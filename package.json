{
  "name": "nest-crud-monorepo",
  "version": "1.0.0",
  "description": "Monorepo for @ackplus/nest-crud and @ackplus/nest-crud-request packages",
  "private": true,
  "scripts": {
    "nest-crud": "pnpm -C packages/nest-crud",
    "nest-crud-request": "pnpm -C packages/nest-crud-request",
    "example": "pnpm -C apps/example-app",
    "build": "pnpm nest-crud-request build && pnpm nest-crud build",
    "publish": "node scripts/publish.js"
  },
  "keywords": [
    "nestjs",
    "crud",
    "typeorm",
    "query-builder",
    "rest-api"
  ],
  "author": "Ackplus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ackplus/nest-crud"
  },
  "packageManager": "pnpm@10.21.0",
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}